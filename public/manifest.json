{
  "name": "LGTM",
  "version": "0.1.0",
  "manifest_version": 3,
  "description": "LGTM",
  "icons": {
    "16": "./images/icon-16.png",
    "48": "./images/icon-48.png",
    "128": "./images/icon-128.png"
  },
  "action": {
    "default_icon": {
      "19": "./images/icon-19.png",
      "38": "./images/icon-38.png"
    },
    "default_popup": "./index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"]
    }
  ]
}
